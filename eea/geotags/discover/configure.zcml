  i18n_domain="eea.geotags">

  <class class=".AlchemyAPI.AlchemyAPI">
    <implements interface=".interfaces.IAlchemyAPI" />
  </class>

  <utility
    provides=".interfaces.IAlchemyAPI"
    factory=".AlchemyAPI.AlchemyAPI" />

  <adapter
    for="eea.geotags.interfaces.IGeoTaggable"
    provides=".interfaces.IDiscoverGeoTags"
    factory="..discover.DiscoverGeoTags"
    />

</configure>
